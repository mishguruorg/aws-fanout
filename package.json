{
  "name": "aws-fanout",
  "version": "1.2.3-0",
  "license": "MIT",
  "description": "A library wrapping SNS and SQS to allow for human readable names when using a fanout technique",
  "main": "dist/index.js",
  "srcPath": "lib",
  "scripts": {
    "build": "pkg-build",
    "flow": "pkg-flow",
    "lint": "pkg-lint",
    "test": "mocha --compilers js:babel-register --recursive specs",
    "prepublish": "rm -rf dist && pkg-build",
    "postpublish": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mishguruorg/aws-fanout"
  },
  "keywords": [
    "SQS SNS"
  ],
  "author": "Mish Guru <dev@mish.guru>",
  "bugs": {
    "url": "https://github.com/mishguruorg/aws-fanout/issues"
  },
  "devDependencies": {
    "@mishguru/package": "^1.8.5",
    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "proxyquire": "^1.7.2",
    "sinon": "^1.16.1"
  },
  "dependencies": {
    "aws-sdk": "^2.2.3",
    "bluebird": "^3.5.0",
    "common-prefix": "^1.1.0",
    "ramda": "^0.24.1"
  },
  "babel": {
    "presets": [
      "flow",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  }
}
