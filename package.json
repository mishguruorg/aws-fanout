{
  "name": "aws-fanout",
  "version": "1.2.3",
  "license": "MIT",
  "description": "A library wrapping SNS and SQS to allow for human readable names when using a fanout technique",
  "main": "dist/index.js",
  "srcPath": "lib",
  "scripts": {
    "prepublish": "pkg-build",
    "build": "pkg-build",
    "flow": "pkg-flow",
    "lint": "pkg-lint",
    "test": "mocha --compilers js:@babel/register --recursive specs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mishguruorg/aws-fanout"
  },
  "keywords": [
    "SQS SNS"
  ],
  "author": "Mish Guru <dev@mish.guru>",
  "bugs": {
    "url": "https://github.com/mishguruorg/aws-fanout/issues"
  },
  "devDependencies": {
    "@mishguru/package": "^4.4.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "proxyquire": "^2.0.1",
    "sinon": "^6.1.3"
  },
  "dependencies": {
    "aws-sdk": "^2.275.1",
    "bluebird": "^3.5.1",
    "common-prefix": "^1.1.0",
    "ramda": "^0.25.0"
  },
  "babel": {
    "presets": [
      "@babel/flow"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ],
      "@babel/plugin-transform-modules-commonjs"
    ]
  }
}
